# This file contains the environment variables for the Trancendos Ecosystem.
# Copy this file to .env and replace the values with your own.

# Database Configuration
# The password for the PostgreSQL database.
POSTGRES_PASSWORD=your_secure_password_here
# The URL for the PostgreSQL database.
DATABASE_URL=postgresql://trancendos:password@localhost:5432/trancendos

# Redis Configuration
# The URL for the Redis server.
REDIS_URL=redis://localhost:6379

# Security Configuration
# The secret key for signing JSON Web Tokens (JWTs).
JWT_SECRET=your_jwt_secret_here
# The key for encrypting sensitive data.
ENCRYPTION_KEY=your_encryption_key_here

# Monitoring Configuration
# The password for the Grafana admin user.
GRAFANA_PASSWORD=your_grafana_password_here

# External Service API Keys
# The API key for the Stripe payment processing service.
STRIPE_API_KEY=your_stripe_api_key_here
# The client ID for the Plaid financial data service.
PLAID_CLIENT_ID=your_plaid_client_id_here
# The secret for the Plaid financial data service.
PLAID_SECRET=your_plaid_secret_here

# Notification Webhooks
# The webhook URL for sending notifications to Slack.
SLACK_WEBHOOK=your_slack_webhook_url_here
# The API key for the Linear issue tracking service.
LINEAR_API_KEY=your_linear_api_key_here

# Notion Integration
# The API key for the Notion integration.
NOTION_API_KEY=your_notion_api_key_here
# The ID of the Notion database to use for the integration.
NOTION_DATABASE_ID=your_notion_database_id_here
